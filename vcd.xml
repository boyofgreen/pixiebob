<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="JacksonCommands">
    <AppName>jackson</AppName>
    <Example>jackson open football section</Example>
    <Command Name="open">
      <Example>jackson section</Example>
      <ListenFor>open {section} [section]</ListenFor>
      <Feedback>opening jackson to {section} section</Feedback>
      <Navigate />
    </Command>
    <PhraseList Label="section">
      <Item>football</Item>
      <Item>cricket</Item>
    </PhraseList>
	  </CommandSet>
</VoiceCommands>